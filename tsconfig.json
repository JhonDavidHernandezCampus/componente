{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./",
    "target": "es5",
    "module": "commonjs",
    "jsx": "preserve",
    "declaration": true,
    "allowJs": true,
    "emitDeclarationOnly": false,
    "declarationMap": false,
    "allowImportingTsExtensions": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/searchProducts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "pages"
  ]
}
